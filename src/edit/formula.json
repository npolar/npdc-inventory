{
  "fieldsets": [{
    "title": "Core",
    "fields": [
      "category",
      "title",
      "description",
      "ephorte_no",
       {
      "id":"draft",
      "title":"Is this entry a draft (internal only)?"
    }
    ]
  },
  {
    "title": "People",
    "fields": [{
      "id": "people",
      "items": {
        "fields": [
            "!id",
          {
            "id": "first_name",
            "title": "first name"
          }, {
            "id": "last_name",
            "title": "last name"
          },
           "organisation",
           {
            "id": "email",
            "title": "email"
           },
          {
            "id": "phone",
            "title": "phone"
          },
           "role"
        ]
      }
    }]
  },
   {
    "title": "Locations",
    "fields": [{
      "id": "locations",
      "items": {
        "fields": [
        "north",
        "east",
        "south",
        "west",
        "placename",
        "place",
        "country",
        "comment"
        ]
      }
    }]
  },
   {
    "title": "Organisations",
    "fields": [{
      "id": "organisations",
      "items": {
        "fields": [{
            "id": "name",
            "title": "organisation name"
          },
          "address",
          "zip",
          "city",
          {
            "id":"@country",
            "title": "country"
          },
          {
            "id": "href",
            "title": "homepage (please include protocol, f.ex 'http://' or 'https://')"
          }
        ]
      }
    }]
  },
   {
    "title": "Dates",
    "fields": [
       {
       "id":"release_dates",
       "title":"release dates"
       },
      {
          "id": "intended_removal_date",
          "title": "intended removal date (if in the future)"
      },
       "comment_date"
    ]
  },
  {
    "title": "Sites",
    "fields": [
      "field_hut",
      "research_station",
      "cultural_heritage",
      "airstrip",
      "heliport",
      {
        "id": "sledeneset_cargo_site",
        "title": "Sledeneset cargo site"
      },
      {
        "id": "stationary_other",
        "title": "other stationaries"
      }, {
        "id": "stationary_quantity",
        "title": "quantity"
      }
    ]
  },
  {
    "title": "Activities",
    "fields": [{
      "id": "rock_samples",
      "title": "rock samples"
    }, {
      "id": "soil_samples",
      "title": "soil samples"
    }, {
      "id": "sediment_samples",
      "title": "sediment samples"
    }, {
      "id": "flora_samples",
      "title": "flora samples"
    }, {
      "id": "interference_fauna_flora",
      "title": "interference fauna/flora"
    }, {
      "id": "introduced_species",
      "title": "introduced species"
    }, {
      "id": "animal_censors",
      "title": "animal censors"
    }, {
      "id": "explosives",
      "title": "explosives"
    }, {
      "id": "radionucleides",
      "title": "radionucleides"
    }, {
      "id": "ice_sampling",
      "title": "ice sampling"
    }, {
      "id": "ice_coring",
      "title": "ice coring"
    }, {
      "id": "snow_sampling",
      "title": "snow sampling"
    }, {
      "id": "air_sampling",
      "title": "air sampling"
    }, {
      "id": "water_sampling",
      "title": "water sampling"
    }, {
      "id": "other_invasive_activity",
      "title": "other invasive activity"
    }]
  },
   {
    "title": "Instruments",
    "fields": [{
      "id": "instrument",
      "items": {
        "fields": [
        {
            "id":"@instrument",
            "title": "instrument"
          },
        "instrument_description",
        "quantity"
        ]
      }
    }]
  },
  {
    "title": "Contamination",
    "fields": [{
      "id": "contamination",
      "items": {
        "fields": [{
            "id": "type",
            "title": "type of contamination"
          },
          "quantity",
          "priority", {
            "id": "impact_likelihood",
            "title": "likelihood of impact"
          }, {
            "id": "impact_spatial",
            "title": "spatial impact"
          }, {
            "id": "impact_temporal",
            "title": "temporal impact"
          }, {
            "id": "impact_documented",
            "title": "Yes, impact has been documented"
          }
        ]
      }
    }]
  },
  {
    "title": "Fuel",
    "fields": [{
      "id": "fuel",
      "items": {
        "fields": [
          "fuel_tank",
          "quantity",
          "content", {
            "id": "drum_full",
            "title": "drum(s) full"
          }, {
            "id": "drum_emptied",
            "title": "drum(s) emptied to be removed"
          },
          {
            "id": "drum_not_extra_protected",
            "title": "drum(s) not protected by extra bounding,double tank, spill carpets etc"
          },

          "expiration_date"
        ]
      }
    }]
  },
  {
    "title": "Links",
    "fields": [{
      "id": "links",
      "items": {
        "fields": [
          "title", {
            "id": "rel",
            "title": "type of link (relation between this registration and the link)"
          }, {
            "id": "href",
            "title": "link (please include protocol f.ex. 'http://' as in link http://www.npolar.no)"
          },
          "title",
          "!hreflang",
          "!type",
          "!modified"
        ]
      }
    }]
  },
{
    "title": "Upload",
    "fields": [{
      "id":"files",
      "title":"Upload documents and photos"
    }]
  },
{
    "title": "Edits",
    "fields": [{
      "id": "created",
      "disabled": true
    }, {
      "id": "created_by",
      "disabled": true
    }, {
      "id": "updated",
      "disabled": true
    }, {
      "id": "updated_by",
      "disabled": true
    },
    {
      "id":"comment",
      "title":"comments for major registration updates"
    }
    ]
  }
]
}